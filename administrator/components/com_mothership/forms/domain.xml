<?xml version="1.0" encoding="UTF-8"?>
<form addfieldprefix="TrevorBice\Component\Mothership\Administrator\Field">
	<field name="id" type="text" default="0" readonly="true" class="readonly" label="JGLOBAL_FIELD_ID_LABEL" description="JGLOBAL_FIELD_ID_DESC" />
	<field name="name" type="domain" default="" readonly="" class="input-xxlarge input-large-text" label="COM_MOTHERSHIP_FIELD_DOMAIN_NAME_LABEL" description="COM_MOTHERSHIP_FIELD_DOMAIN_NAME_DESC" required="true"/>
	<field name="client_id" type="clientlist" default="" readonly="" class="" label="COM_MOTHERSHIP_FIELD_CLIENT_ID_LABEL" description="COM_MOTHERSHIP_FIELD_CLIENT_NAME_DESC" required="true"/>
	<field name="account_id" type="accountlist" default="" readonly="" class="" label="COM_MOTHERSHIP_FIELD_ACCOUNT_ID_LABEL" description="COM_MOTHERSHIP_FIELD_ACCOUNT_NAME_DESC" required="true"/>
	<field name="status" type="domainstatus" default="" readonly="" class="" label="COM_MOTHERSHIP_FIELD_DOMAIN_STATUS_LABEL" description="COM_MOTHERSHIP_FIELD_DOMAIN_STATUS_DESC" />
	<field name="registrar" type="registrar" default="" readonly="" class="" label="COM_MOTHERSHIP_FIELD_DOMAIN_REGISTRAR_LABEL" description="COM_MOTHERSHIP_FIELD_DOMAIN_REGISTRAR_DESC" />
	<field name="dns_provider" type="dns" default="" readonly="" class="" label="COM_MOTHERSHIP_FIELD_DOMAIN_DNS_PROVIDER_LABEL" description="COM_MOTHERSHIP_FIELD_DOMAIN_DNS_PROVIDER_DESC" />
	<field name="ns1" type="text" default="" readonly="" class="" label="COM_MOTHERSHIP_FIELD_DOMAIN_NS1_LABEL" description="COM_MOTHERSHIP_FIELD_DOMAIN_NS1_DESC" />
	<field name="ns2" type="text" default="" readonly="" class="" label="COM_MOTHERSHIP_FIELD_DOMAIN_NS2_LABEL" description="COM_MOTHERSHIP_FIELD_DOMAIN_NS2_DESC" />
	<field name="ns3" type="text" default="" readonly="" class="" label="COM_MOTHERSHIP_FIELD_DOMAIN_NS3_LABEL" description="COM_MOTHERSHIP_FIELD_DOMAIN_NS3_DESC" />
	<field name="ns4" type="text" default="" readonly="" class="" label="COM_MOTHERSHIP_FIELD_DOMAIN_NS4_LABEL" description="COM_MOTHERSHIP_FIELD_DOMAIN_NS4_DESC" />
	<field name="purchase_date" type="calendar" readonly="false" class="validate-date" label="COM_MOTHERSHIP_FIELD_DOMAIN_PURCHASE_DATE_LABEL" description="COM_MOTHERSHIP_FIELD_DOMAIN_PURCHASE_DATE_DESC" default="" format="%Y-%m-%d" filter="user_utc" />
	<field name="expiration_date" type="calendar" readonly="false" class="validate-date" label="COM_MOTHERSHIP_FIELD_DOMAIN_EXPIRATION_DATE_LABEL" description="COM_MOTHERSHIP_FIELD_DOMAIN_EXPIRATION_DATE_DESC" default="" format="%Y-%m-%d" filter="user_utc" />
	<field name="auto_renew" type="checkbox" default="0" readonly="" class="" label="COM_MOTHERSHIP_FIELD_DOMAIN_AUTO_RENEW_LABEL" description="COM_MOTHERSHIP_FIELD_DOMAIN_AUTO_RENEW_DESC" />
	<field name="notes" type="textarea" default="" readonly="" class="input-xxlarge input-large-text" label="COM_MOTHERSHIP_FIELD_DOMAIN_NOTES_LABEL" description="COM_MOTHERSHIP_FIELD_DOMAIN_NOTES_DESC" />
	<field name="created" type="calendar" readonly="false" class="validate-date" label="COM_MOTHERSHIP_FIELD_DOMAIN_CREATED_LABEL" description="COM_MOTHERSHIP_FIELD_DOMAIN_CREATED_DESC" default="" format="%Y-%m-%d" filter="user_utc" />
</form>
